    var Utility = (function() {
        function Utility() {}
        Utility.getCookies = function(key) {
            var value = $.cookie(RC.Setting.ServerDomain + key);
            return value;
        };

        Utility.log = function(msg) {
            // log to console
            if (RC.Setting.EnableLog) {
                if (window.console) {
                    console.log(msg);
                }
            }
        };

        Utility.remoteLog = function(logitem) {
            try {
                var url = RC.Setting.GetLogAPIUrl();
                if (Utility.isNullOrEmpty(url)) {
                    Utility.log('logapi url is null or empty.');
                } else {
                    Utility.jsonp(url, logitem, function() {
                        Utility.log('send error log to remote successfully.');
                    });
                }
            } catch (err) {
                Utility.log(err);
            }
        };

        Utility.remoteLogError = function(error, msg, targetId) {
            var logitem = new LogItem();
            // MODIFY: 推送客户端错误数据时，修改PC中日志source为4
            logitem.Source = 4;

            try {
                if (!Utility.isNull(RongIM.imUser.userId)) {
                    logitem.FromUserID = RongIM.imUser.userId;
                }

                logitem.Error = !Utility.isNull(error) ? error : '';
                logitem.Message = !Utility.isNull(msg) ? msg : '';
                logitem.ToUserID = !Utility.isNull(targetId) ? targetId : 0;

                Utility.remoteLog(logitem);
            } catch (e) {
                Utility.log(e);
            }
        }

        Utility.isNullOrEmpty = function(value) {
            return (typeof(value) == 'undefined' || value == null || value.length == 0);
        };

        Utility.isNull = function(value) {
            return (typeof(value) == 'undefined' || value == null);
        };

        Utility.jsonp = function(url, data, succ) {
            $.ajax({
                dataType: "jsonp",
                url: url,
                data: data,
                success: succ,
                error: function(xhr, status, error) {
                    Utility.log(error);
                }
            });
        };

        Utility.rendering = function(template, value) {
            var html = template.replace(/\{([\w\.]+)\}/g, function(match, exp) {
                var items = exp.split(".");
                var v = Utility.getValue(exp, value);
                return v;
            });

            return html;
        };

        Utility.getValue = function(exp, value) {
            var endRegex = new RegExp("\\\)$");

            if (Utility.isNull(exp)) {
                value = null;
            } else {
                var exps = exp.split(".");
                for (var i = 0; i < exps.length && !Utility.isNull(value); i++) {
                    var subexp = exps[i];
                    if (Utility.isNullOrEmpty(subexp)) {
                        value = null;
                    } else if (subexp == 'this') {
                        value = value;
                    } else if (endRegex.test(subexp)) {
                        value = eval("data." + subexp);
                    } else {
                        value = value[subexp];
                    }
                }
            }

            return Utility.isNull(value) ? '' : value;
        };

        Utility.getChatSource = function() {
            try {
                if ("undefined" == typeof(Global_IM_ChatSource))
                    return 4;
                return Global_IM_ChatSource;
