        RongIM.getConvList = function(sessions) {
            var dtd = $.Deferred();

            var list = sessions;
            if (list != null) {
                Utility.log('CONV_LIST:' + list.length);

                list = list.sort(function(a, b) {
                    if (a.updateTime < b.updateTime)
                        return -1;

                    if (a.updateTime > b.updateTime)
                        return 1;

                    return 0;
                }).reverse();

                // filter system notice, undefined conv
                var convlist = new Array();
                for (var i = 0; i < list.length; i++) {
                    var item = list[i];

                    if (item.to == null || item.to == '0' || item.to == 'undefined' || item.to.indexOf('-') == 0) {
                        continue;
                    }

                    //对于非点对点的特殊消息暂不予处理
                    /*
                    if (item.scene != 'p2p') {
                        continue;
                    }
                    */
                    convlist.push(item);
                }

                for (var i = 0; i < convlist.length; i++) {
                    var item = convlist[i];
                    var lastMsgContent = item.lastMsg.content && JSON.parse(item.lastMsg.content);
                  
                    var conv = new IMConv();
                    conv.contactId = item.to;
                    conv.convType = item.scene;
                    conv.lastSentTime = item.updateTime;
                    conv.unreadCount = item.unread;
                    conv.sessionId = item.id;
                    conv.lastMsgId = 0;
                    if(lastMsgContent) {
                      conv.unitId = lastMsgContent.unitId;
                      conv.hotelId = lastMsgContent.hotelId;
                    }
                    conv.hasDetectMessageCode = 1; //code:1,表示为检测语言;code:2,表示正在检测;code:0,表示语言检测成功


                    RongIM.dataManager.updateContactConv(item.to, conv);
                }

                //首次默认加载第一页
                if (RC.Setting.IsTuJingWeb) {
                    RongIM.LoadMoreAvatar();
                } else {
                    RongIM.uiWebIM.showContacts();

                    var chatIds = [], teamIds = [];
                    // refresh contact info
                    $.map(RongIM.dataManager.getAll(), function(item) {
                        if(item.scene != 'team') {
                          // 非群聊场景，会话中User中userId即为会话中的B端用户Id
                          chatIds.push(item.User.userId);
                        } else {
                          teamIds.push(item.User.userId);
                        }
                    });
                    RongIM.RefreshAvatar_2(chatIds, teamIds);
                }

                dtd.resolve();
            } else {
                Utility.log("获取会话列表失败");
                dtd.reject();
            }
            RongIM.checkTotalUnreadMsgCount();
            return dtd;
        };
