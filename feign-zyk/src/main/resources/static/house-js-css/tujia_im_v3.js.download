        UIWebIM.prototype.displayMsg = function(contact, msg) {
            var targetId = null;
            if (contact != null) {
                targetId = contact.Id;
            }

            var send; // true表示C端用户发送消息，false表示B端用户发送消息
            // 来自群聊, 只要不是用户发的消息，都是B端商户发的消息
            if(contact.scene == 'team') {
              if( msg.senderUserId != window.TujiaIM.userId) {
                send = false;
              } else {
                send = true;
              }
            } else {
              send = msg.senderUserId != contact.Id
            }
            var html, msgEle;
            if( msg.messageType == 13) { // 群成员变更文本消息
              html = UIWebIM.HtmlTemplate.groups_chat_contact_member_change;
              msgEle = $(html);
              msgEle.find('span').html(msg.text);
            } else {
              html = UIWebIM.HtmlTemplate.groups_chat_msg_container;
              msgEle = $(html);
              if (send) {
                  msgEle.find('img.chatuser-pic').attr('src', RC.RongIM.imUser.avatar);
                  msgEle.addClass('tjim-chateme');
              } else {
                  msgEle.addClass('tjim-chattouser');
                  msgEle.attr('chatuser', msg.senderUserId);
                  if(contact.scene == 'team' && contact.members) {
                    var member = contact.members.filter(function(member) {
                      return member.chatId == msg.senderUserId
                    })[0];
                    msgEle.find('img.chatuser-pic').attr('src', (member && member.avatar) || '');
                    // if(!(member && member.avatar)) {
                      // TODO: 头像容错
                      msgEle.find('.chatuser-pic').error(function() {
                        $(this).attr('src', defaultMerchantAvatar);
                      });
                    // }
                  } else {
                    msgEle.find('img.chatuser-pic').attr('src', contact.User.avatar);
                  }
              }
  
              var msgContent = null;
              switch (msg.messageType) {
                  case 1:
                      msgContent = UIWebIM.RenderingRichContentMessage(msg);
                      break;
                  case 0:
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
                  case 7:
                      msg.text = '不支持的消息，请在最新版途家app上查看';
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
                  case 8:
                      msg.text = '不支持的消息，请在最新版途家app上查看';
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
                  case 9:
                      msg.text = '我刚刚来电咨询，但是没有接通。';
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
                  case 10:
                      msg.text = '不支持的消息，请在最新版途家app上查看';
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
                  default:
                      msg.text = '不支持的消息，请在最新版途家app上查看';
                      msgContent = UIWebIM.RenderingTextMessage(msg);
                      break;
              }
  
              //msgEle.children('.tjim-chatuser').append((new Date(msg.sentTime)).format('yyyy-MM-dd hh:mm:ss'));
              msgEle.children('.tjim-chatuser').append(msgContent);
              //return;
            }

            var view = this.getUIChatView(targetId);
            var viewEle = view[0];

            var bottomOffset = viewEle.scrollHeight - viewEle.clientHeight - viewEle.scrollTop;
            // if bottom less than 300, treate it as zero
            var keepBottom = false;
            if (bottomOffset < 300) { keepBottom = true; }

            var list = view.children('ul').first();
            // UI层操作消息本身和时间分割内容
            appendMsgWithSpliter(list, contact, msg, msgEle);

            function appendMsgWithSpliter(list, contact, msg, msgEle) {
              var time = new Date(msg.sentTime);
              var spliter = RC.Utility.rendering(UIWebIM.HtmlTemplate.groups_chat_msg_spliter, time.format('yyyy-MM-dd hh:mm'));
              if (contact.FirstMsgTime == null && contact.LastMsgTime == null) {
                  // append
                  $(spliter).appendTo(list);
                  contact.FirstMsgTime = contact.LastMsgTime = msg.sentTime;
                  msgEle.appendTo(list);
              } else if (msg.sentTime < contact.FirstMsgTime) {
                  // prepend
                  msgEle.prependTo(list);
                  if (contact.FirstMsgTime - msg.sentTime >= 60 * 1000) {
                      $(spliter).prependTo(list);
                  }
                  contact.FirstMsgTime = msg.sentTime;
              } else if (msg.sentTime > contact.LastMsgTime) {
                  // append
                  if (msg.sentTime - contact.LastMsgTime >= 60 * 1000) {
                      $(spliter).appendTo(list);
                  }
                  contact.LastMsgTime = msg.sentTime;
                  msgEle.appendTo(list);
                  msgEle[0].scrollIntoView(false);
                  keepBottom = true;
              } else {
                  //无插入
                  console.log('无插入msg=', msg, ' FirstMsgTime=' + contact.FirstMsgTime, ' LastMsgTime=' + contact.LastMsgTime)
              }
            }

            if ((this.getActiveContactId() != contact.Id) || this.isHide()) {
                contact.isNeedShowBottom = keepBottom;
            } else {
                var topOffset = (viewEle.scrollHeight - viewEle.clientHeight) - (keepBottom ? 0 : bottomOffset);
                view.scrollTop(topOffset);
            }
        };

