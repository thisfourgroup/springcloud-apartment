    /* 
     * 消息敏感词检查器
     * 1、目前只用于检查强敏感词，强敏感词消息不能发送
     * 2、弱敏感词服务端检查后发送静默推送，客户端负责解析并弹出提示信息
     */
    RC.MessageInspector = function(_getWordsUrl) {
        this.inspect = function(msg) {
            var words = RC.MessageInspector.StrongWords;
            if (!words || words.length == 0) {
                return null;
            }
            //过滤字符串中的空格制表符等
            msg = msg.replace(/\s/g, "");
            //检查所有的强敏感词库，如有不符合则返回不符合的敏感词
            for (var i = 0; i < words.length; i++) {
                if (words[i] && words[i].filter && words[i].filter.test(msg)) {
                    words[i].filter.lastIndex = 0
                    return words[i];
                }
            }
            return null;
        }

        //读取服务端敏感词列表
        if (_getWordsUrl && "" != _getWordsUrl) {
            $.ajax({
                type: "GET",
                url: _getWordsUrl,
                dataType: "json",
                success: function(_words) {
                    console.log(_words);
                    if (_words && _words.Keywords && _words.Keywords.length > 0) {
                        RC.MessageInspector.ServerKeyWords = _words.Keywords;
                    }
                }
            });
        }
    }

    //敏感词基类定义
    RC.MessageInspector.Word = function() {
        this.filter = null;
        this.msg = "";
    }

    //强敏感词定义
    RC.MessageInspector.StrongWord = function() {}
    $.extend(true, RC.MessageInspector.StrongWord.prototype, RC.MessageInspector.Word);

    RC.MessageInspector.ServerKeyWords = [];

    //强敏感词词库
    RC.MessageInspector.StrongWords = [{
            //连续的数字位数大于9位
            filter: {
                test: function(msg) {
                    // 将途家URL部分替换为空
                    msg = msg.replace(/((http(s?):\/\/)?)(([a-z0-9\-]+)\.)*tujia\.com(\/[!-~]+)*/ig, "");

                    // 检测是否有连续的10位数字
                    return (/([0-9一二三四五六七八九十零壹贰叁肆伍陆柒捌玖拾０１２３４５６７８９][\s\-\－]*){10,}/i).test(msg);
                }
            },
            msg: Resources && Resources.tujingIM.TuJiaChatTip ? Resources.tujingIM.TuJiaChatTip : "途家小二提醒您，站外交易有风险，建议您通过途家交易"
        },
        {
            //敏感词（该敏感词列表从服务端获取）
            filter: {
                test: function(msg) {
                    //根据从服务器获取的敏感词关键字拼正则检查
                    var keyWords = RC.MessageInspector.ServerKeyWords;
                    if (keyWords && keyWords.length > 0) {
                        var reg = new RegExp(keyWords.join("|"), "ig");
                        return reg.test(msg);
                    }
                    return false;
                }
            },
            msg: Resources && Resources.tujingIM.TuJiaChatTip ? Resources.tujingIM.TuJiaChatTip : "途家小二提醒您，站外交易有风险，建议您通过途家交易"
        },
        {
            //非途家域名的链接检查（文本中混合的连接和不以http开头的连接）
            //此种复杂校验，需自定义检查逻辑
            filter: {
                test: function(msg) {
                    var matches = msg.match(/((http(s?):\/\/)?)(([a-z0-9\-]+)\.)+(com|net|cn)/ig); //查找消息中的所有连接字符串
                    if (matches) {
                        for (var i = 0; i < matches.length; i++) {
                            var match = matches[i];
                            var items = match.split(/\.|\//ig);
                            var domain = items.slice(items.length - 2, items.length).join(".");

                            if (!(/^(tujia)\.com$/i).test(domain)) {
                                return true;
                            }
                        }
                        return false;
                    }
                    return false;
                }
            },
            msg: Resources && Resources.tujingIM.TuJiaChatTip ? Resources.tujingIM.TuJiaChatTip : "途家小二提醒您，站外交易有风险，建议您通过途家交易"
        }
    ];

