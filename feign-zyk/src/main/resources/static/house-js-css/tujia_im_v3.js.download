                        //if (nComplete == nPageCount) {
                        //如果所有的页面加载完成，展示用户信息
                        //RongIM.uiWebIM.showContacts();
                        //}
                    });
                }
                for (var n = 0; n < nPageCount; n++) {
                    var nLast = (n + 1) * nPageSize;
                    var items = idlist.slice(n * nPageSize, nTotalCount <= nLast ? nTotalCount : nLast);

                    var data = {
                        ChatUserIDs: items.join(','),
