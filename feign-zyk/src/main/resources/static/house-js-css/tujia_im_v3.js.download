function bindIMDialogClick($dialogEntry) {
    $dialogEntry.bind('click', function(event) {
        if (!TujiaIM.isLogin) {
            showLoginDialog();
            return;
        }

        var ownerId = $(this).attr('ownerId') - 0,
            name = $(this).attr('ownerName'),
            unitId = $(this).attr('unitId'),
            hotelId = $(this).attr('hotelId'),
            sessionType = $(this).attr('sessionType'),
            chatteamid = $(this).attr('chatteamid');

        if (!RC.RongIM.imUser || (ownerId != RC.RongIM.imUser.userId)) {
            var user = {
                ChatUserId: ownerId,
                UserName: name
            };
            // 新旧房屋做兼容，设置hotelId
            RC.RongIM.CurrentUnit.hotelId = hotelId;
            RC.RongIM.CurrentUnit.sessionType = sessionType;
            RC.RongIM.CurrentUnit.unitId = unitId;
            if(sessionType == 1) {
              RC.RongIM.SetCurrentUnit(chatteamid + '', unitId);
            } else {
              RC.RongIM.SetCurrentUnit(ownerId + '', unitId);
            }
            RC.RongIM.show(function() {
                if(sessionType == 1) {
                  RC.RongIM.chat(chatteamid, true, 1);
                } else {
                  RC.RongIM.chat(ownerId, true, 0);
                }
            });
            //TujiaIM.ContactManager.refreshUser(user, true);
            //TujiaIM.UI.showTalkDialog(ownerId);
        } else {
            alert("不能和自己聊天！")
        }
        event.stopPropagation();
    });
}

