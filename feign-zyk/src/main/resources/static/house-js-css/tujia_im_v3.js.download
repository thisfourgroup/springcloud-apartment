            var key = this.getContactKey(contactId);
            var contact = this._dataProvider[key] = new Contact();

            contact.Id = contactId;
            contact.User = new IMUser();
            contact.Conv = new IMConv();

            contact.User.userId = contactId;
            contact.User.name = contactId;
