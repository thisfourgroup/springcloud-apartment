                        $('<div class="u-translate-tx">' + translateLogo + '</div>').insertAfter(chatSay.parent().parent())
                    }
                },
                complete: function() {
                    chatSay.removeClass('z-translate-loading');
                    chatSay.addClass('z-translate-complete')
                }
            })
        }

        RongIM.sendMessage = function(targetId, content, callback) {
            var contact = RongIM.dataManager.findContact(targetId);

            if (typeof callback === "undefined") { callback = null; }

            //如果targetId是：0，负数，1000000000，则记录日志到服务端
            if (typeof targetId == "undefined" || targetId <= 0 || targetId == 1000000000) {
                var srcE = typeof window.event != "undefined" ? (window.event.srcElement || window.event.target) : null;
                Utility.log(srcE);
                if (srcE) {
                    srcE = srcE.tagName == "INPUT" ? srcE.value : srcE.text;
                }
                TujiaIM.MessageFetcher.logMessage(RongIM.imUser.userId, targetId, msg, srcE);
                Utility.log("当前会话用户异常");
                return;
            };
            // 更新聊天中的unitId
            content.unitId = getUnitId(contact, RongIM.CurrentUnit);

            /**
             * 返回消息最终携带的UnitId
             * @param {Object} contact 当前聊天会话
             * @param {Object} currentUnit RongIM.CurrentUnit 当前会话对应的房屋信息
             */
            function getUnitId(contact, currentUnit) {
              
              if(contact.Id == currentUnit.contactId && currentUnit.unitid && currentUnit.unitid != 0) {
                return currentUnit.unitid;
              } else {
                return contact.unitId;
              }
            }

            // 根据content中是否包含text或title设置其pushContent
            var pushContent = '';
            if ('text' in content) {
                pushContent = content.text;
            } else if ('title' in content) {
                pushContent = content.title;
            }
            var pushPayload = {
                type: 1,
                items: {
                    data: encodeURIComponent(JSON.stringify(content))
                }
            };
            var msg = RongIM.NIM.sendCustomMsg({
                scene: contact.scene,
                to: targetId,
                content: JSON.stringify(content),
                pushPayload: JSON.stringify(pushPayload),
                pushContent: pushContent,
                done: sendMsgDone
            });

            function sendMsgDone(error, msg) {
                if (!error) {
                    console.log(msg.sessionId + ":::" + targetId)
                    RongIM.NIM.resetSessionUnread(msg.sessionId);
                    if (callback != null) {
                        callback();
                    }
                    var amsg = JSON.parse(msg.content)
                    amsg.senderUserId = msg.from;
                    //amsg.sentTime = RC.Utility.isNullOrEmpty(amsg.sendTime) ? msg.time : amsg.sendTime;
                    amsg.sentTime = msg.time;
                    var contact = RongIM.dataManager.getContact(targetId);
                    RongIM.uiWebIM.displayMsg(contact, amsg);
                    RongIM.setTop(contact.Id);
                    Utility.log(msg);
                } else {
                    if(error.code == 803) {
                      RongIM.uiWebIM.showWarningTip(targetId, '此会话已超过聊天时限，请从房屋页或订单页重新发起会话');
                    } else if(error.code == 7101) {
                      console.log(msg.sessionId + ":::" + targetId)
                        RongIM.NIM.resetSessionUnread(msg.sessionId);
                        if (callback != null) {
                            callback();
                        }
                        var amsg = JSON.parse(msg.content)
                        amsg.senderUserId = msg.from;
                        //amsg.sentTime = RC.Utility.isNullOrEmpty(amsg.sendTime) ? msg.time : amsg.sendTime;
                        amsg.sentTime = msg.time;
                        var contact = RongIM.dataManager.getContact(targetId);
                        RongIM.uiWebIM.displayMsg(contact, amsg);
                        RongIM.uiWebIM.showSystemTips(targetId, '消息已发出，但被对方拒收了')
                        RongIM.setTop(contact.Id);
                        Utility.log(msg);
                    } else if(error.code == 423) {
                      console.log(msg.sessionId + ":::" + targetId)
                        RongIM.NIM.resetSessionUnread(msg.sessionId);
                        if (callback != null) {
                            callback();
                        }
                        var amsg = JSON.parse(msg.content)
                        amsg.senderUserId = msg.from;
                        //amsg.sentTime = RC.Utility.isNullOrEmpty(amsg.sendTime) ? msg.time : amsg.sendTime;
                        amsg.sentTime = msg.time;
                        var contact = RongIM.dataManager.getContact(targetId);
                        RongIM.uiWebIM.displayMsg(contact, amsg);
                        RongIM.uiWebIM.showSystemTips(targetId, '由于涉嫌违规，您的聊天权限暂时被冻结，请您稍后再试！')
                        RongIM.setTop(contact.Id);
                        Utility.log(msg);
                    } else {
                      RongIM.uiWebIM.showWarningTip(targetId, '消息发送失败。请再次发送，或刷新页面后重试');
                    }
                    var info = 'ERROR:' + error;
                    Utility.log('发送失败:' + info);
                    error = '发送失败' + ' Message:' + error.message;
                    Utility.remoteLogError(error, msg.content, targetId);
                }
            }
        };
