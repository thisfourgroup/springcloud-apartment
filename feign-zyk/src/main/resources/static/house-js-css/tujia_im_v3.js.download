        UIWebIM.RenderingTextMessage = function(msg) {
            if (RC.Utility.isNull(msg)) return;
            var msgContent = $(UIWebIM.HtmlTemplate.groups_chat_msg_text);
            //翻译 add
            msgContent.attr("im_messageId", msg.messageId);
            msgContent.attr("im_messageType", msg.messageType);
            msgContent.attr("im_senderUserId", msg.senderUserId);
            //end

            var text = '';
            if (!RC.Utility.isNull(msg.text)) {
                text += msg.text;
            }

            // 检测是否有途家URL
            var urlRegex = /((http(s?):\/\/)?)(([a-z0-9\-]+)\.)*tujia\.com(\/[!-~]+)*/i

            var match = text.match(urlRegex);
            while (match) {
                var url = match[0];
                var length = url.length;

                var start = match.index;
                var stop = start + length - 1;

                if (start > 0) {
                    msgContent.append($('<span class="u-chat-text">').text(text.substr(0, start)));
                }

                var href = url;
                var hasSchema = href.match(/http(s?):\/\//i);
                if (!hasSchema) {
                    href = 'http://' + href;
                }
                var link = $('<a>').text(url)
                    .attr('href', href)
                    .attr('target', '_blank')
                    .attr('title', '途家');
                msgContent.append(link);

                text = text.substr(stop + 1, text.length - (stop + 1));
                match = text.match(urlRegex);
            }
            msgContent.append($('<span>').text(text));

            return msgContent;
        }

        UIWebIM.RenderingRichContentMessage = function(msg) {
