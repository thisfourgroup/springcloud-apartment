        /**
         * 过滤掉attach类型消息
         * @param {Object} msgs 云信消息列表
         */
        RongIM.filterAttachMessage = function(msgs) {
          if(msgs && msgs.length > 0) {
            return msgs.filter(function(item, index, arr) {
              return !item.attach;
            });
          } else {
            return [];
          }
        }

        // NOTE: 判断是否需要根据contact修改消息发送接收人的id
        RongIM.displayMsgs = function(imMsgs, contact, isSortAsc) {
            // 需要针对attach类型消息进行过滤
            var list = RongIM.filterAttachMessage(imMsgs);
            var initRoamingMsgIndex = list.length > 20 ? list.length - 20 : 0;
            var hasMsg = true;
            if (list == null || list.length == 0) {
                hasMsg = false;
            }
            if (contact.HasHistoryMsg == null || contact.HasHistoryMsg) {
                contact.HasHistoryMsg = hasMsg;
            }
            //TODO: 判断其他更新历史消息时候是否需要过滤attach类型消息
            RongIM.uiWebIM.updateHistoryLink(contact);

            if (list == null || list.length == 0) {
                return;
            } else {
                var msgs = [];
                for (var i = initRoamingMsgIndex; i < list.length; i++) {
                  var r = new RegExp(','+ list[i].type + ',')
                  if ( r.test(',' + ['custom', 'image'].join(',')+ ',') == -1 ) {
                      Utility.log('非法消息：' + list[i]);
                      continue;
                  }
                  if (list[i] != null) {
                      // NOTE: 消息过滤
                      if (this.messageMatchConversation(list[i])) {
                        if(list[i].content) {
                          var msg = JSON.parse(list[i].content)
                          msg.senderUserId = list[i].from;
                          //msg.sentTime = Number(RC.Utility.isNullOrEmpty(msg.sendTime) ? list[i].time : msg.sendTime);
                          msg.sentTime = list[i].time;
                          msg.getHistoryEndTime = list[i].time;
                          msg.messageId = Number(list[i].idServer);
                          msgs.push(msg);
                              //取最小的time 以及 idServer
                              //if (!contact.endTime || contact.endTime > msg.sentTime) {
                              //    contact.endTime = msg.sentTime
                          if (!contact.endTime || contact.endTime > msg.getHistoryEndTime) {
                              contact.endTime = msg.getHistoryEndTime;
                              contact.lastMsgId = list[i].idServer;
                              console.log('设置endTime=' + contact.endTime, ' lastMsgId=' + contact.lastMsgId)
                          }
                        } else if(list[i].custom) {
                          var msg = JSON.parse(list[i].custom)
                          msg.senderUserId = list[i].from;
                          msg.sentTime = list[i].time;
                          msg.getHistoryEndTime = list[i].time;
                          msg.messageId = Number(list[i].idServer);
                          msgs.push(msg);
                          if (!contact.endTime || contact.endTime > msg.getHistoryEndTime) {
                            contact.endTime = msg.getHistoryEndTime;
                            contact.lastMsgId = list[i].idServer;
                            console.log('设置endTime=' + contact.endTime, ' lastMsgId=' + contact.lastMsgId)
                          }
                        }
                      }
                  }
              }

                msgs = msgs.sort(function(a, b) {
                    if (isSortAsc) {
                        return a.sentTime > b.sentTime ? 1 : -1;
                    } else {
                        return a.sentTime < b.sentTime ? 1 : -1;
                    }
                });
                for (var j = 0; j < msgs.length; j++) {
                    RongIM.uiWebIM.displayMsg(contact, msgs[j]);
                }
            }
        }

        //切换聊天时获取消息sourceLnaguage
        RongIM.SetChatContactMessageDetectIcon = function(contactId) {
            var currContact = RongIM.dataManager.getContact(contactId);
            //currContact.hasDetectMessageCode = 2;
            var transMsgs = [];
            if (!!currContact && currContact.hasDetectMessageCode == 1) {
                currContact.hasDetectMessageCode = 2;
                var $msgList = $("div.tjim-chatview.tjim-scrollbar[im-chatid='" + contactId + "']").find('div.tjim-chatsay');
                $.each($msgList, function(index, item) {
                    var isReceiveMsg = $(item).attr("im_senderUserId") == contactId;
                    //var isReceiveMsg = true;
                    if ($(item).attr("im_messagetype") == 0 && isReceiveMsg) {
                        var transMsg = {};
                        transMsg.messageID = Number($(item).attr("im_messageid"));
                        transMsg.message = $(item).text();
                        transMsgs.push(transMsg);
                    }
                })
            }
            if (transMsgs != null && transMsgs.length > 0) {
                RongIM.GetChatLanguageDetect(transMsgs, contactId);
            }
        }

        //获取消息历史记录时获取消息sourceLanguage
        RongIM.SetHistoryMessageDetectIcon = function(contactId, msgs) {
                var transMsgs = [];
                if (!!msgs && msgs.length > 0) {
                    for (var i = 0; i < msgs.length; i++) {
                        if (!!msgs[i] && msgs[i].content) {
                            var transMsg = {};
                            var isReceiveMsg = msgs[i].from == contactId;
                            var msgContent = JSON.parse(msgs[i].content);
                            if (msgContent.messageType == 0 && isReceiveMsg) {
                                transMsg.messageID = Number(msgs[i].idServer);
                                transMsg.message = msgContent.text;
                                transMsgs.push(transMsg);
                            }
                        }
                    }
                }
                if (transMsgs != null && transMsgs.length > 0) {
                    RongIM.GetChatLanguageDetect(transMsgs, contactId);
                }

            }
            //检测当前每条消息的sourceLanguage
        RongIM.GetChatLanguageDetect = function(transMsgs, contactId) {
            var currContact = RongIM.dataManager.getContact(contactId);

            var data = {};
            var url = RC.Setting.GetChatLanguageDetectUrl();
            data.userID = RongIM.tjUser.loginId;
            data.userToken = RongIM.tjUser.loginToken;
            data.messages = transMsgs;
            $.ajax({
                crossDomain: true,
                url: url,
                data: JSON.stringify(data),
                contentType: 'application/json',
                dataType: "json",
                type: "post",
                success: function(result) {
                    var transMsgList = [];
                    if (result && result.code == 0 && result.messages && result.messages.length > 0) {
                        currContact.hasDetectMessageCode = 0;
                        var msgs = result.messages;
                        for (var i = 0; i < msgs.length; i++) {
                            if (msgs[i] && msgs[i].code == 0) {
                                var msg = {};
                                msg.messageID = msgs[i].messageID;
                                msg.message = msgs[i].message;
                                msg.sourceLanguage = msgs[i].lang;
                                msg.isNeedTranslate = msgs[i].lang != RC.Setting.TuJingLanguage;
                                msg.isExistZH = msgs[i].lang == "zh" || RC.Setting.TuJingLanguage == "zh";
                                transMsgList.push(msg);
                            }
                        }
                    }
                    if (!!transMsgList && transMsgList.length > 0) {
                        RongIM.displayTranslateIcon(transMsgList);
                    }
                }
            });
        }

        //显示聊天翻译入口
        RongIM.displayTranslateIcon = function(transMsgList) {
            var translate = RC.Setting.IsTuJingWeb ? Resources.tujingIM.Translate : "翻译";
            for (var i = 0; i < transMsgList.length; i++) {
                var msg = transMsgList[i];
                if (msg.isNeedTranslate && msg.isExistZH) {
                    $("div.tjim-chatsay[im_messageid='" + msg.messageID + "']").attr("sourceLanguage", msg.sourceLanguage)
                    $("div.tjim-chatsay[im_messageid='" + msg.messageID + "']").append("<span class='u-translate'><span class='u-translate-text'>" + translate + "</span></span>")
                        //<span><img src='../Images/Im/translate-loading.gif'></span>
                }
            }
        }

        //会话消息翻译
        RongIM.translateChat = function(messageId, sourceLanguage, sourceText) {
            //切换翻译文字到loading
            var translateLogo = RC.Setting.IsTuJingWeb ? Resources.tujingIM.TranslateLogo : "腾讯翻译君翻译";
            var chatSay = $("div.tjim-chatsay[im_messageid='" + messageId + "']")
            chatSay.addClass('z-translate-loading')
            var url = RC.Setting.GetChatLanguageTranslateUrl();
            var data = {};
            data.userID = RongIM.tjUser.loginId;
            data.userToken = RongIM.tjUser.loginToken;
            data.source = sourceLanguage;
            data.sourceText = sourceText;
            data.target = RC.Setting.TuJingLanguage;
            $.ajax({
                crossDomain: true,
                url: url,
                data: JSON.stringify(data),
                contentType: 'application/json',
                dataType: "json",
                type: "post",
                success: function(result) {
                    if (!!result && result.code == 0) {
                        //var chatSay = $("div.tjim-chatsay[im_messageid='" + messageId + "']")
                        chatSay.append("<div class='u-translate-content'>" + result.targetText + "</div>");
