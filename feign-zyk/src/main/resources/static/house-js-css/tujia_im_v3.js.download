        /**
         * 判断消息和当前用户是否匹配
         * @param {message} message
         * @return {boolean} 
         */
        RongIM.messageMatchConversation = function(message) {
                if (message.content || message.custom) {
                    var messageContent = JSON.parse(message.content||message.custom);
                    // 根据targetType判断新旧消息体，新消息体进行判断，旧消息体默认不过滤
                    if (messageContent.targetType == 0 || messageContent.targetType == 1) {
                        // 当接收方为游客，发送给自己或者群主为自己
                        if (messageContent.targetType == 0 && (message.to == RongIM.imUser.userId || messageContent.ownerId == RongIM.imUser.userId)) {
                            return true;
                        }
                        // 当接收方为商户，来自自己
                        if (messageContent.targetType == 1 && message.from == RongIM.imUser.userId) {
                            return true;
                        }
                        return false;
                    }
                }
                return true;
            }
            /**
             * 过滤不符合规则的会话
             * @param {session} session
             * @return {boolean} 
             */
        RongIM.filterSession = function(session) {
            var lastMsg = session.lastMsg;
            if (lastMsg && (lastMsg.content || lastMsg.custom )) {
              var messageContent = JSON.parse(lastMsg.content || lastMsg.custom);
                // 根据targetType判断新旧消息体，新消息体进行判断，旧消息体默认不过滤
                if (messageContent.targetType == 0 || messageContent.targetType == 1) {
                    // 当接收方为游客，发送给自己或者群主为自己
                    if (messageContent.targetType == 0 && (lastMsg.to == RongIM.imUser.userId ||
                      messageContent.ownerId == RongIM.imUser.userId)) {
                        return true;
                    }
                    // 当接收方为商户，来自自己
                    if (messageContent.targetType == 1 && lastMsg.from == RongIM.imUser.userId) {
                        return true;
                    }
                    return false;
                }
                return true;
            }
            return true;
        }

        /**
         * 更新会话的unitId字段
         * @param {Object|Array[Object]} msgs 云信消息
         * @param {Object} contact 对应会话
         */
        RongIM.updateContactUnitId = function(msgs, contact) {
          if(Object.prototype.toString.call(msgs).replace(/\[object /, '').replace(/]$/, '') == 'Array') {
            // 倒序拿取
            for(var i = msgs.length - 1; i >= 0; i--) {
              var msgContent = msgs[i].content && JSON.parse(msgs[i].content);
              if(msgContent && msgContent.unitId && msgContent.unitId != 0 && contact) {
                contact.unitId = msgContent.unitId;
                break;
              }
            }
          } else {
            var content = msgs.content && JSON.parse(msgs.content);
            if(content && content.unitId && content.unitId != 0 && contact) {
              contact.unitId = content.unitId;
            }
          }
        }

        RongIM.onReceiveMessage = function(msgIm) {
            if (RongIM.isShow && RongIM.CurrentContact && RongIM.CurrentContact.sessionId == msgIm.sessionId) {
                RC.RongIM.NIM.resetSessionUnread(RongIM.CurrentContact.sessionId);
            }
            var msg = JSON.parse(msgIm.content||msgIm.custom);
            // NOTE: 接受消息时，使用target而非to,to可能为自己
            var targetId = msgIm.target;
            msg.senderUserId = msgIm.from
                //msg.sentTime = RC.Utility.isNullOrEmpty(msg.sendTime) ? msgIm.time : msg.sendTime;
            msg.sentTime = msgIm.time;
            msg.messageId = Number(msgIm.idServer);

            var isNotice = msg.senderUserId < 0;

            var contact = RongIM.dataManager.findContact(targetId);
            // NOTE: 处理过滤, 不符合过滤规则不做任何操作
            if (!this.messageMatchConversation(msgIm)) {
                return;
            }

            if (contact == null) {
                contact = RongIM.dataManager.getContact(targetId);
                if (!isNotice) {
                    //为用户提前加载数据
                    RongIM.history(targetId);
                }
            }

            RongIM.updateContactUnitId(msgIm, contact);

            //对于senderUserId < 0 的特殊消息(系统消息，活动通知等)，刷新系统未读通知数量
            if (isNotice) {
                //如果用户一直没有登录过，则登录后会收到若干历史通知，在此为了避免高频重复的触发该逻辑，做特殊处理
                if (window._LoadUnReadNoticeEvent) {
                    clearTimeout(window._LoadUnReadNoticeEvent);
                }

                window._LoadUnReadNoticeEvent = setTimeout(function() {
                    if (TujiaIM.MessageFetcher && TujiaIM.MessageFetcher.loadUnReadNoticeCount) {
                        TujiaIM.MessageFetcher.loadUnReadNoticeCount();
                    }
                }, 1000);
                return;
            }

            RongIM.setTop(targetId, function() {
                RongIM.uiWebIM.displayMsg(contact, msg);
            });

            //对接收消息进行语言判断
            var transMsgs = [];
            transMsgs.push(msgIm);
            RongIM.SetHistoryMessageDetectIcon(targetId, transMsgs);

            alert4newMessage();
        };

        RongIM.onReceiveUpdateContact = function(session) {
            //如果不存在 直接置顶
            var isHasConv = RongIM.dataManager.findContact(session.to)
                //转化为contact 并更新contact
            var conv = RongIM.dataManager.getContact(session.to)
            conv.convType = session.scene;
            conv.lastSentTime = session.updateTime;
            conv.unreadCount = session.unread;
            conv.sessionId = session.id; // conv的sessionId必须带scene, 例: p2p-123
            conv.contactId = session.to;
            var lastMsgContent = session.lastMsg.content && JSON.parse(session.lastMsg.content);
            if(lastMsgContent && lastMsgContent.unitId && lastMsgContent.unitId != 0) {
              conv.unitId = lastMsgContent.unitId;
            }
            if (session.lastMsg.hotelId) {
                conv.hotelId = session.lastMsg.hotelId;
            }
            //conv.lastMsgId = session.lastMsgId; //这里不需要设置
            RongIM.dataManager.updateContactConv(session.to, conv);
            //2.置顶contact
            if (!isHasConv) {
                RongIM.setTop(session.to);
            }
            //3.更新unread
            RongIM.changeConvUnreadMsgCount(session.to, session.unread);
            RongIM.checkTotalUnreadMsgCount();
        };

