        // contactId C端用户聊天Id或者群聊teamId
        RongIM.chat = function(contactId, istop, sessionType) {
            // 1.获取当前会话

            if(sessionType == 1) {
              // 群聊,更新群状态
              $.ajax(
                RC.Setting.updateChatTeamState(),
                {
                  data: {
                    UnitId: RongIM.CurrentUnit.unitId,
                    Source: 4,
                    OwnerChatId: RongIM.imUser.userId,
                    LoginToken: RongIM.tjUser.loginToken,
                    LoginUserId: RongIM.tjUser.loginId,
                    TeamId: contactId || '' // 群聊下，contactId等于chatroomId
                },
                  type: 'post',
                  success: function(result) {
                    // console.error('更新群组状态' + result.TeamInfo.TeamId || '' + ',只在新建群聊时有用，开始聊天后请忽略此错误提示');
                    if(result.IsSuccess) {
                      if(!contactId.trim().length) {
                        contactId = result.TeamInfo.TeamId;
                        RongIM.CurrentUnit.contactId = contactId; // 手动更新当前会话的会话Id
                      }
                      handleFn(contactId, sessionType);
                    } else {
                      if(result.ErrorCode == 1) { // 未登录状态
                        if(window.tjAlert) {
                          window.tjAlert({
                            content: '请重新登录',
                            cancel: '',
                            mask: true
                          });
                        } else {
                          alert('请重新登录');
                        }
                      }
                    }
                  }
                }
              )
            } else {
              handleFn(contactId, sessionType);
            }
            function handleFn(contactId, sessionType) {
              var contact = RongIM.dataManager.getContact(contactId, sessionType);
              // 2.NIM设置当前会话
              //if (!contact.sessionId) {
              //    RongIM.NIM.setCurrSession(contact.sessionId)
              //}
              //else {
              //    contact.sessionId = 'p2p-' + contact.targetId;
              //}
              //设置当前会话Id
              RongIM.CurrentContact = contact
              RongIM.NIM.setCurrSession(contact.sessionId)
                  // 3.设置会话UI
              RongIM.uiWebIM.chat(contact, istop);
              // 4.获取消息语言类型
              RongIM.SetChatContactMessageDetectIcon(contactId);
  
              // 5.刷新头像
              if (contact.Id == contact.User.name && !contact.IsLoaded) {
                  if(sessionType == 1) {
                    RongIM.RefreshAvatar_2(null, [contact.Id]);
                  } else {
                    RongIM.RefreshAvatar_2([contact.Id]);
                  }
              }
              // 6.获取历史消息
              if (contact.FirstMsgTime == null && contact.LastMsgTime == null) {
                  RongIM.history(contact.Id);
              }
            }

        };

        RongIM.setTop = function(contactId, callback) {
