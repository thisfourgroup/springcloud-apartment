// 动态生成二维码有关代码

function getUTM() {
  if (window.tjGlobal) {
      return window.tjGlobal.tujia_utm;
  }
}

function getQRURL() {
  var utmValue = getUTM();
  if (utmValue) { // 返回值非空，表示存在utm_source
      // 生成二维码
      return 'http://go.tujia.com/1039/?code=downapp&link=sempchome';
  }
  return;
}

// 左下角浮动层
function createQRcodeInFloatLayer() {
  var QRURL = getQRURL();
  if (QRURL) {
      if ($('#popfloating_float_img').length) { // 判断该节点是否存在,不存在则不做操作
          if (!($('#popfloating_float_img>canvas').length)) { // 未生成动态二维码
              // 删除包含的img标签，设置好二维码容器位置,及其中心logo
              $('#downloadappimg').remove();
              // 消除之前的样式
              $('#popfloating_float_img').css({
                  'background-image': 'none',
                  'background-position': '0% 0%',
              });
              QRCode.wechatQr = new QRCode(document.getElementById('popfloating_float_img'), {
                  width: 115,
                  height: 115
              });
              QRCode.wechatQr.makeCode(QRURL);
              $('#popfloating_float_img').addClass('dynamic_QRcode');
              // 设置样式
              $('#popfloating_float_img>img').css({
                  'border': '3px solid #FFF',
              });
          }
      }
  }
}


// 右下im内
function createQRcodeInToolBar() {
  var QRURL = getQRURL();
  if (QRURL) {
      if ($('.sprite_toolBar_app').length) { // DOM节点存在
          if (!($('.sprite_toolBar_app>canvas').length)) { // 未生成动态二维码
              // 下右im内
              //清除background-image 和background-position
              $($('.sprite_toolBar_app')[0]).css({
                  'background-image': 'none',
                  'background-position': '0% 0%',
              });
              QRCode.wechatQr = new QRCode(document.getElementsByClassName('sprite_toolBar_app')[0], {
                  width: 140,
                  height: 140
              });
              QRCode.wechatQr.makeCode(QRURL);
              $('.sprite_toolBar_app').addClass('dynamic_QRcode');
          }
      }
  }
}

// u-icon-app-down的父元素设置relative定位,u-icon-app-down:after设置relative定位，image设置
function createQRcodeInFooter() {
  var QRURL = getQRURL();
  if (QRURL) { // URL中存在utm_source
      if ($('.u-icon-app-down').length) { // DOM节点存在
          if (!($('.u-icon-app-down>canvas').length)) { // 未生成动态二维码
              //清除background-image 和background-position
              $('.u-icon-app-down').css({
                  'background-image': 'none',
                  'background-position': '0% 0%',
              });
              QRCode.wechatQr.makeCode(QRURL);
              $('.u-icon-app-down').addClass('dynamic_QRcode');
              // 设置二维码div中img为block
              $('.u-icon-app-down.dynamic_QRcode').css({
                  'padding': '7px',
                  'background': '#FFF'
              });
          }
      }
  }
}

// 此时脚本必然加载完毕，生成动态二维码即可
createQRcodeInFooter();